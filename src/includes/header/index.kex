<%/*
  it.home: string = '/',
  it.logo: html = '',
  it.navElements: {name: string, href: string}[]
*/%>
<% const homeHref = it.home || '/'; %>

<div class="header">
  <nav class="header__nav">
    <a class="header__logo" href=<%= homeHref  %> >
      <%~ it.logo || '' %>
    </a> 
    <% it.navElements.forEach((navElement) => { %>
      <a class="header__link" href=<%= navElement.href %> ><%=navElement.name%></a>
    <% }) %>
  </nav>
</div>

<%/*
  it.pageIndex: number
  it.totalPage: number
*/%>

<% 
  const {pageIndex, totalPage} = it
%>

<div class="paginator">
  <% for (let index = 1; index <= totalPage; index++){ %>
    <span class="paginator__item">
      <a class="paginator__link" href=<%= '/page/' + index %> >
        <%= index %>
      </a>
    </span>
  <% } %>

</div>

